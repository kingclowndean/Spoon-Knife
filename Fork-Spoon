<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/8647573029773076915/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/8647573029773076915/comments/default";
          
          holder.currentBlogUrl = "http://notdeadyet-mysorrowfulmother.blogspot.com/";
          holder.currentBlogId = "8647573029773076915";
        
        return holder;
      });
    }
  </script>
